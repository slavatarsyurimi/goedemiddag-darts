{
    "preset": "laravel",
    "rules": {
        "ordered_imports": {
            "sort_algorithm": "length"
        },
        "concat_space": {
            "spacing": "one"
        },
        "binary_operator_spaces": {
            "operators": {
                "=>": "align_single_space_minimal"
            }
        },
        "not_operator_with_successor_space": false,
        "cast_spaces": {
            "space": "none"
        },
        "yoda_style": {
            "equal": false,
            "identical": false,
            "less_and_greater": false
        },
        "nullable_type_declaration": {
            "syntax": "union"
        },
        "ordered_types": {
            "null_adjustment": "always_last"
        }
    }
}
